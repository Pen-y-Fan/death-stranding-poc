{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://pen-y-fan.github.io/death-stranding-poc/schemas/district.schema.json",
  "title": "District",
  "type": "object",
  "additionalProperties": false,
  "required": ["id", "name", "region"],
  "properties": {
    "id": { "type": "integer", "minimum": 1 },
    "name": { "type": "string", "minLength": 1 },
    "region": { "type": "string", "enum": ["East", "Central", "West"] }
  }
}
